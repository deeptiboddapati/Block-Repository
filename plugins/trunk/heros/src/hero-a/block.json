{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "toms/hero-a",
	"version": "0.1.0",
	"title": "Hero A",
	"category": "design",
	"icon": "cover-image",
	"description": "Hero Image, Headline, Supporting Copy, Conversion button, places to buy.",
	"supports": {
		"html": false
	},
	"attributes": {
		"headline": {
			"type": "string",
			"source": "html",
			"selector": ".toms-hero-a-headline"
		},
		"supportingcopy": {
			"type": "string",
			"source": "html",
			"selector": ".toms-hero-a-supportingcopy"
		},
		"heroimage": {
			"type": "string",
			"source": "attribute",
			"selector": ".toms-hero-a-heroimage",
			"attribute": "src"
		},
		"heroimagealt": {
			"type": "string",
			"source": "attribute",
			"selector": ".toms-hero-a-heroimage",
			"attribute": "alt",
			"default": ""
		},
		"heroimageid": {
			"type": "number"
		},
		"marketplaceimageone": {
			"type": "string",
			"source": "attribute",
			"selector": ".toms-hero-a-marketplaceimageone",
			"attribute": "src"
		},
		"marketplaceimageonealt": {
			"type": "string",
			"source": "attribute",
			"selector": ".toms-hero-a-marketplaceimageone",
			"attribute": "alt",
			"default": ""
		},
		"marketplaceimageoneid": {
			"type": "number"
		},
		"marketplaceimagetwo": {
			"type": "string",
			"source": "attribute",
			"selector": ".toms-hero-a-marketplaceimagetwo",
			"attribute": "src"
		},
		"marketplaceimagetwoalt": {
			"type": "string",
			"source": "attribute",
			"selector": ".toms-hero-a-marketplaceimagetwo",
			"attribute": "alt",
			"default": ""
		},
		"marketplaceimagetwoid": {
			"type": "number"
		}
	},
	"textdomain": "hero-a",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}