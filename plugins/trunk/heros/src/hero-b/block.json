{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "toms/hero-b",
	"version": "0.1.0",
	"title": "Hero B",
	"category": "design",
	"icon": "cover-image",
	"description": "Hero Image, Headline, Supporting Copy, Conversion button, places to buy.",
	"supports": {
		"html": false
	},
	"attributes": {
		"headline": {
			"type": "string",
			"source": "text",
			"selector": ".toms-hero-b-headline"
		},
		"supportingcopy": {
			"type": "string",
			"source": "text",
			"selector": ".toms-hero-b-supportingcopy"
		},
		"heroimage": {
			"type": "string",
			"source": "attribute",
			"selector": ".toms-hero-b-heroimage",
			"attribute": "src"
		},
		"heroimagealt": {
			"type": "string",
			"source": "attribute",
			"selector": ".toms-hero-b-heroimage",
			"attribute": "alt",
			"default": ""
		},
		"heroimageid": {
			"type": "number"
		},
		"color": {
			"type": "string"
		}
	},
	"textdomain": "hero-b",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}